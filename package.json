{
  "name": "rc-ui",
  "version": "0.1.9",
  "description": "基于React Hooks和TypeScript开发的移动端组件库",
  "main": "lib/index.js",
  "private": true,
  "author": "rancui",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:rancui/vant-react.git"
  },
  "keywords": [
    "react",
    "react hooks",
    "react-ui",
    "react-component",
    "react-mobile",
    "react-mobile-library",
    "component",
    "components",
    "framework",
    "frontend team"
  ],
  "dependencies": {
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-dev-server": "^3.11.3",
    "classnames": "^2.2.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "standard-version": "^9.3.0"
  },
  "scripts": {
    "serve": "webpack-dev-server --progress --colors --config webpack.dev.js",
    "build": "rm -rf lib && webpack --progress --colors --config webpack.prod.js",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
