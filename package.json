{
  "name": "rc-ui-lib",
  "version": "0.1.24",
  "description": "基于React Hooks和TypeScript开发的移动端组件库",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "typings": "cjs/index.d.ts",
  "private": false,
  "author": "rancui<271242038@qq.com>",
  "files": [
    "cjs",
    "es"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rancui/rc-ui-lib.git"
  },
  "keywords": [
    "react",
    "react hooks",
    "rc-ui",
    "rc-ui-lib",
    "react-ui",
    "react component",
    "react-mobile",
    "react-mobile-library",
    "ui component",
    "components",
    "ui framework",
    "frontend team"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs && yarn build:umd",
    "build:esm": "rimraf es && cross-env NODE_ENV=production BABEL_ENV=esm rollup -c && yarn build:typed --outDir es",
    "build:cjs": "rimraf cjs && cross-env NODE_ENV=production BABEL_ENV=cjs rollup -c && yarn build:typed --outDir cjs",
    "build:umd": "rimraf umd && cross-env NODE_ENV=production BABEL_ENV=umd rollup -c",
    "build:typed": "tsc --declaration --emitDeclarationOnly --noEmit false",
    "check:tsc": "tsc",
    "lint-staged": "cross-env ESLINT_ENV=commit eslint --fix --ext .js,.ts,.tsx",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "bugs": {
    "url": "https://github.com/rancui/rc-ui-lib/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-function-sent": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-transition-group": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "autoprefixer": "^10.2.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^10.0.0",
    "git-cz": "^4.7.6",
    "less": "^4.0.0",
    "lint-staged": "^10.5.3",
    "node-sass": "^6.0.0",
    "postcss": "^8.2.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.49.0",
    "rollup-plugin-styles": "^3.12.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.34.0",
    "standard-version": "^9.3.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "classnames": "^2.3.1"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "sideEffects": [
    "umd/*",
    "es/**/*.css",
    "cjs/**/*.css",
    "*.scss"
  ]
}
